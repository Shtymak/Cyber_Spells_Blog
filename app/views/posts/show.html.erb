<h3><%= link_to "All Posts",  posts_path %> 
<%= link_to "Home", home_path%> 
<%= link_to "Edit", edit_post_path(@post)%> 
<%= link_to "Delete",post_path(@post), method: :delete%></h3>
<h1><%=@post.title%></h1>
<h4><%=@post.body%></h4>
<%= image_tag @post.photo, width:300 %>
<div id="images">
<%@post.images.each do |image|%>
<%=image_tag image, width:200%>
<%end%>
</div>